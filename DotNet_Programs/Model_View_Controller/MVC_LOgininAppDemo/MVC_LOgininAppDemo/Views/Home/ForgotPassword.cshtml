@model MVC_LOgininAppDemo.Models.UserProfile

@TempData["Name"]
@using (Html.BeginForm())
        {
    @Html.AntiForgeryToken()
<table>
    <tr><td>UserName</td><td>@Html.TextBox("Username")</td></tr>
    <tr><td>New Password</td><td>@Html.TextBox("Userpassword")</td></tr>
    <tr><td>:</td><td><input type="submit" value="submit"/></td></tr>
</table> 

            }


<div>
    @{ 
        if(ViewBag.Message != null)
        {
            @ViewBag.Message;
        }
    }

    @Html.ActionLink("Back to Login","Login")
</div>



